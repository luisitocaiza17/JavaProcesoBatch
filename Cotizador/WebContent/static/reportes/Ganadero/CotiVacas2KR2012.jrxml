	<subDataset name="Dataset1" uuid="5247f498-fabc-45ed-bb6e-b608fde142c5">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="COTIZADOR"/>
		<parameter name="Cot_Id" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT ENT.NOMBRE_COMPLETO AS NOMBRE
FROM ENTIDAD ENT INNER JOIN AGENTE AG
ON ENT.ID = AG.ENTIDAD_ID INNER JOIN COTIZACION COT
ON AG.ID = COT.AGENTE_ID
and COT.ID =  $P{Cot_Id}]]>
		</queryString>
		<field name="NOMBRE" class="java.lang.String"/>
	</subDataset>
	<parameter name="Cot_Id" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT IFNULL(ENT.NOMBRE_COMPLETO,'')AS NOMBRE_COMPLETO,
CONCAT( IFNULL(S.NOMBRE,' '),', ',CAST(COT.FECHA_ELABORACION AS CHAR)) AS LUGARYFECHA,
IFNULL(S.NOMBRE,'') AS SUCURSAL
FROM ENTIDAD ENT INNER JOIN CLIENTE CLI
ON ENT.ID = CLI.ENTIDAD_ID INNER JOIN COTIZACION COT
ON CLI.ID = COT.CLIENTE_ID INNER JOIN COTIZACION_DETALLE COTD
ON COT.ID = COTD.COTIZACION_ID INNER JOIN PUNTO_VENTA PV
ON COT.PUNTO_VENTA_ID = PV.ID INNER JOIN SUCURSAL S 
ON PV.SUCURSAL_ID = S.ID
and COT.ID = $P{Cot_Id}]]>
	</queryString>
	<field name="NOMBRE_COMPLETO" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="LUGARYFECHA" class="java.lang.String"/>
	<field name="SUCURSAL" class="java.lang.String"/>
