	<property name="com.jaspersoft.studio.unit." value="cm"/>
	<parameter name="COTIZACION_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select C.numeroFactura FACTURA,
A.nombre_completo AGENTE,
( SELECT R.nombre FROM RAMO R, PRODUCTO PRO WHERE C.producto_id=PRO.id AND R.id=PRO.ramo_id ) RAMO,
ifnull(D.calle_principal,'')  calle_principal,
ifnull(D.calle_secundaria,'') CALLE_SECUNDARIA,
ifnull(D.numero,'')  NUMERO_CASA,
E.nombre_completo CLIENTE_NOMBRE,
E.identificacion CLIENTE_IDENTIFICACION,
( SELECT distinct CONT.id_ensurance FROM PRODUCTO_X_PUNTO_VENTA PPV, CONTENEDOR CONT WHERE CONT.id=PPV.contenedor_id AND C.grupo_por_producto_id=PPV.grupo_por_producto_id AND C.punto_venta_id=PPV.punto_venta_id ) CONTENEDOR,
(  VP.valor*365 ) VIGENCIA_DIAS,
C.suma_asegurada_total SUMA_ASEGURADA,
C.prima_neta_total PRIMA,
C.imp_iva IMP_IVA,
C.imp_derecho_emision IMP_DE,
C.imp_recargo_seguro_campesino IMP_RSC,
C.imp_seguro_campesino IMP_SC,
C.imp_super_bancos IMP_SB,
C.vigencia_desde VIGENCIA_DESDE,
DATE_ADD(C.vigencia_desde, INTERVAL VP.VALOR YEAR) VIGENCIA_HASTA,
(C.prima_neta_total+C.imp_derecho_emision+C.imp_recargo_seguro_campesino +C.imp_seguro_campesino +C.imp_super_bancos ) SUB_TOTAL,
(SELECT distinct FP.nombre FROM FORMA_PAGO FP WHERE PA.forma_pago_id=FP.id ) FORMA_PAGO,
(SELECT distinct CUO.VALOR FROM CUOTAS CUO WHERE PA.id=CUO.pago_id) CUOTA,
PA.plazon_en_mes PLAZO,
(C.totalFactura*C.porcentaje_comision/100) COMISION,
C.porcentaje_comision POR_COMISION,
C.id COTIZACION_ID,
U.telefono_natural TELEFONO_NATURAL,
U.telefono_empresa TELEFONO_EMPRESA,
U.celular_natural CELULAR_NATURAL,
C.totalFactura TOTAL,
E.telefono TELEFONO
   FROM
    COTIZACION C
        LEFT JOIN
    CLIENTE CLI ON C.cliente_id = CLI.id 
        LEFT JOIN
    ENTIDAD E ON E.id = CLI.entidad_id
 LEFT JOIN
    AGENTE AGE ON AGE.id = C.agente_id
LEFT JOIN
    ENTIDAD A ON AGE.entidad_id = A.id
LEFT JOIN
    UPLA U ON CLI.id = U.cliente_id
LEFT JOIN
    DIRECCION D ON D.id = U.direccion_id
LEFT JOIN
    PAGO PA ON PA.id = C.pago_id
LEFT JOIN
    VIGENCIA_POLIZA VP ON VP.id = C.vigencia_poliza_id
WHERE C.id= $P{COTIZACION_ID} ;]]>
	</queryString>
