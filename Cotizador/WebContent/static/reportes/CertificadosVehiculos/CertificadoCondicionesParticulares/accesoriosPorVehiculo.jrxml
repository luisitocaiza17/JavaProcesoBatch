	<parameter name="VEHICULO" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    TE.nombre,
    E.descripcion,
    E.valor_asegurado,
    (select 
            count(*)
        from
            EXTRAS
        where
            objeto_vehiculo_id = OV.id) cuantos
FROM
    TIPO_EXTRA TE,
    EXTRAS E,
    OBJETO_VEHICULO OV
WHERE
    E.objeto_vehiculo_id = OV.id
        AND TE.id = E.tipo_extra_id
	  AND OV.id= $P{VEHICULO}]]>
	</queryString>
